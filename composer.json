{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-zip": "*",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1",
        "ably/ably-php": "^1.1",
        "predis/predis": "^2.0",
        "pixel-softwares-com/pixel-app": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "App\\CustomLibs\\" : "app/CustomLibs/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Pixel-Softwares-com/temporary-files-handlers.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Pixel-Softwares-com/exp-imp-management.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Pixel-Softwares-com/morphic-abstract-eloquent.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Pixel-Softwares-com/authorization-management.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Pixel-Softwares-com/validator-lib.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Pixel-Softwares-com/custom-file-system.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Pixel-Softwares-com/file-info-data-manager.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Pixel-Softwares-com/crud-services.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Pixel-Softwares-com/runtime-caching.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Pixel-Softwares-com/custom-generators.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Pixel-Softwares-com/data-resource-instructors.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Pixel-Softwares-com/statistics.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Pixel-Softwares-com/pixel-dom-pdf.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Pixel-Softwares-com/pixel-app.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Pixel-Softwares-com/pixel-macroable-extenders.git"
        }
    ]
}
